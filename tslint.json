{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "rules": {
    "array-type": false,
    "arrow-parens": false,
    "await-promise": [ true, "Client", "Request", "PromiseLike" ],
    "callable-types": false,
    "interface-name": false,
    "max-classes-per-file": [ true, 2 ],
    "max-line-length": false,
    "member-access": false,
    "member-ordering": false,
    "no-bitwise": false,
    "no-console": false,
    "no-empty": false,
    "no-empty-interface": false,
    "no-floating-promises": [ true, "Client", "PromiseLike" ],
    "no-for-in-array": true,
    "no-implicit-dependencies": [ true, "dev" ],
    "no-invalid-template-strings": true,
    "no-internal-module": false,
    "no-namespace": false,
    "no-return-await": true,
    "no-shadowed-variable": false,
    "no-trailing-whitespace": [ true, "ignore-comments" ],
    "no-unsafe-any": false,
    "no-unused-expression": [ true, "allow-fast-null-checks", "allow-new" ],
    "no-unused-variable": [ true, "check-parameters" ],
    "no-var-keyword": false,
    "no-var-requires": false,
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "prefer-const": false,
    "prefer-for-of": false,
    "quotemark": false,
    "radix": false,
    "trailing-comma": false,
    "variable-name": false,
    "whitespace": [ true, "check-branch", "check-decl", "check-module", "check-operator", "check-rest-spread", "check-separator", "check-type", "check-typecast", "check-type-operator", "check-preblock" ]
  },
  "rulesDirectory": [
  ]
}
